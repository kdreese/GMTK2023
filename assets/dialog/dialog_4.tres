[sub_resource type="Resource" id="Resource_p1ga0"]
script = ExtResource("1_3ans1")
speaker = "Military Advisor"
text = "In war, you have to decide how to allocate your resources."

[sub_resource type="Resource" id="Resource_y6vqv"]
script = ExtResource("1_3ans1")
speaker = "Military Advisor"
text = "This card can be used for either [b]attack[/b] or [b]defense[/b], but not both. Choose wisely!"

[sub_resource type="Resource" id="Resource_h1bdr"]
script = ExtResource("1_3ans1")
speaker = "Military Advisor"
text = "You can also choose not to use this card and save it for later."

[sub_resource type="Resource" id="Resource_t3w0u"]
script = ExtResource("1_3ans1")
speaker = "Military Advisor"
text = "If you're unsure about what a card does, drag it onto the help icon in the bottom left corner."

[sub_resource type="Resource" id="Resource_6ps5f"]
script = ExtResource("1_3ans1")
speaker = "Military Advisor"
text = "Once you've made your decision, don't forget to press the [b]end turn[/b] button. Godspeed."

